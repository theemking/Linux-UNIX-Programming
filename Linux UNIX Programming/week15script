#[8CAssignment: Create a script called lookup to display datafile in sorted order

#preparing condition to exit script

#checking for the file
#if [ ! -e datafile ]
#if [ ! -w datafile ]
#then
#echo "file not found"
#printf "datafile was not found, check user permission "
#exit 1
#fi
#echo

# b. Sort datafile by last names in order
echo "The contact from datafile sorted by last name"

# sortting codes
echo
sort -k 2 datafile.txt
echo
# C show the user the contents of the datafile
echo "Below is the content of the file"
cat datafile.txt
echo

entry=$(cat datafile.txt | wc -l)
printf "The number of entries in the datafile are: $entry"
echo

# Prompting user to add and entry to the datafile
while true; do
read -p "Would you like to add an entry to the datafile: " yn
case $yn in
[Yy]* ) break;;
[Nn]* ) exit;;
* ) echo "Please select yes or no.";;
esac
done


#adding menu to the lookup scrip
function menu()
{
printf "Menu"
echo "[1] Add entry"
echo "[2] Delete entry"[1;1H[?25h


















































[?25l[1;52r[52;1H
[1;53r[52;1Hecho "[3] View entry"[53;1H[K[52;1H[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hecho "[4] Exit"[?25h[?25l[1;52r[52;1H
[1;53r[52;1H}[?25h[?25l[1;52r[52;1H
[1;53r[52;1H[?25h[?25l[1;52r[52;1H
[1;53r[52;1H#adding function to the lookup scrip[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hfunction add()[?25h[?25l[1;52r[52;1H
[1;53r[52;1H{[?25h[?25l[1;52r[52;1H
[1;53r[52;1H[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hecho "Please enter the person first and last name: "[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hread Firstname Lastname[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hif grep -iq "$Firstname $Lastname" datafile; then[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hprintf "$Firstname $Lastname already exist in the datafile. \n"[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hreturn 1;[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hfi[?25h[?25l[1;52r[52;1H
[1;53r[52;1H[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hecho "Please enter the phone number: "[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hread phone[?25h[?25l[1;52r[52;1H
[1;53r[52;1H# To remove unwanted character so that only digits will be accepted[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hphone = $(echo "$phone" | sed 's/[^0-9]*//g')[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hphone = $(echo "${phone:0:3} - ${phone:3:3}-${phone:6:4}")[?25h[?25l[1;52r[52;1H
[1;53r[52;1H[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hecho "Please enter the address: "[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hread address[?25h[?25l[1;52r[52;1H
[1;53r[52;1H[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hecho "Please enter the birthday (mm/dd/yyyy): "[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hread birthday[?25h[?25l[1;52r[52;1H
[1;53r[52;1H[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hecho "Please enter the salary"[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hread salary[?25h[?25l[1;52r[52;1H
[1;53r[52;1H[?25h[?25l[1;52r[52;1H
[1;53r[52;1H# To remove unwanted character so that only digits will be accepted[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hsalary=$(echo "$salary" | sed 's/[^0-9]*//g')[?25h[?25l[1;52r[52;1H
[1;53r[52;1H#append user input to the datafile[?25h[?25l[1;52r[52;1H
[1;53r[52;1H#echo "$Firstname $Lastname:$address:$birthday:$salary" >> datafile.txt[?25h[?25l[1;52r[52;1H
[1;53r[52;1H[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hecho "$Firstname $Lastname:$phone:$address:$birthday:$salary" >> datafile.txt[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hprintf "$Firstname $Lastname has been added to the datafile."[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hgrep -inF"$Firstname $Lastname" datafile.txt[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hecho[?25h[?25l[1;52r[52;1H
[1;53r[52;1H}[?25h[?25l[1;52r[52;1H
[1;53r[52;1H[?25h[?25l[1;52r[52;1H
[1;53r[52;1H#setting up the delete function[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hfunction delete()[?25h[?25l[1;52r[52;1H
[1;53r[52;1H{[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hecho "Enter the first and last name for the entry you want to delete"[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hread Firstname Lastname[?25h[?25l[1;52r[52;1H
[1;53r[52;1Hif grep -iq "$Firstname $Lastname" datafile; then[?25h[?25l[1;52r[52;1H
